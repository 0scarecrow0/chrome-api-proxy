  "type": "module",
  "scripts": {
    "build": "pnpm uno:build && tsc -b && vite build",
    "format": "prettier --write \"./src/pages/**/*.{html,js,jsx,ts,tsx,json}\"",
    "lint": "eslint \"./src/**/*.{html,js,jsx,ts,tsx,json}\" --fix --max-warnings=0",
    "prepare": "husky",
    "start": "nodemon",
    "stylelint": "stylelint \"./src/pages/**/*.{css,less}\" --fix",
    "uno:build": "unocss \"./src/pages/**/*\" -o ./src/assets/styles/uno.css"
